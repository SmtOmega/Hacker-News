    if(nextPage > state.nbPages -1){\r\n          nextPage = 0\r\n        }\r\n        return {...state, page: nextPage}\r\n      }\r\n      if(action.payload === \"dec\") {\r\n        let prevPage = state.page -1\r\n        if(prevPage < 0){\r\n          prevPage = state.nbPages -1\r\n        }\r\n        return { ...state, page: prevPage }\r\n       }\r\n      \r\n    default:\r\n      throw new Error(`no matching ${action.type}`);\r\n  }\r\n};\r\nexport default reducer;\r\n","C:\\Users\\Shola\\Desktop\\Practise\\webdev_prac\\udemy1\\react_proj\\Hacker-News\\src\\Stories.js",[],"C:\\Users\\Shola\\Desktop\\Practise\\webdev_prac\\udemy1\\react_proj\\Hacker-News\\src\\actions.js",[],{"ruleId":"39","replacedBy":"40"},{"ruleId":"41","replacedBy":"42"},{"ruleId":"43","severity":1,"message":"44","line":43,"column":5,"nodeType":"45","messageId":"46","endLine":44,"endColumn":53},"no-native-reassign",["47"],"no-negated-in-lhs",["48"],"no-fallthrough","Expected a 'break' statement before 'default'.","SwitchCase","default","no-global-assign","no-unsafe-negation"]